{
  "name": "alpine-mousedrag-plugin",
  "version": "0.2.0",
  "description": "An Alpine.js plugin for mouse drag with inertia",
  "type": "module",
  "main": "dist/alpine-mousedrag-plugin.cjs",
  "module": "dist/alpine-mousedrag-plugin.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/alpine-mousedrag-plugin.js",
      "require": "./dist/alpine-mousedrag-plugin.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "alpinejs",
    "plugin",
    "mousedrag",
    "inertia"
  ],
  "author": "Jonas List",
  "license": "MIT",
  "dependencies": {
    "@types/alpinejs": "^3.13.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "alpinejs": "^3.14.9",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "alpinejs": "^3.0.0"
  }
}
